import random

num = random.randint(1, 5)
num2 = int(input("enter a number 1 - 5"))
if num2 == num:
  print("well done")
elif num > num2:
  print("too small")
elif num < num2:
  print("too high")
num2 = int(input("enter a number 1 - 5"))
if num2 != num:
  print("you loose")
else:
  print("correct")
